{
  // See https://go.microsoft.com/fwlink/?linkid=830387
  // for the documentation about the launch.json format
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run TSLint Tests",
      "program": "${workspaceFolder}/node_modules/.bin/tslint",
      "args": [
        "--test",
        "test/rules/**/*"
      ],
      "preLaunchTask": "Compile",
      "console": "internalConsole",
      "outputCapture": "std"
    }
  ]
}
