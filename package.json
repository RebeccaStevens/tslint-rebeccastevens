{
  "name": "@rebeccastevens/tslint-rebeccastevens",
  "version": "0.1.6",
  "description": "TSLint rules I made for myself.",
  "main": "tslint-rebeccastevens.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RebeccaStevens/tslint-rebeccastevens.git"
  },
  "author": "Rebecca Stevens <rebecca.stevens@outlook.co.nz>",
  "license": "BSD-3-Clause",
  "keywords": [
    "tslint"
  ],
  "bugs": {
    "url": "https://github.com/RebeccaStevens/tslint-rebeccastevens/issues"
  },
  "homepage": "https://github.com/RebeccaStevens/tslint-rebeccastevens#readme",
  "scripts": {
    "compile": "yarn generate-tslint-files && yarn build",
    "generate-tslint-files": "ts-node -P tsconfig.tools.json scripts/generate-tslint-files.ts",
    "build": "del rules; rollup -c",
    "lint": "yarn lint:src && yarn lint:other",
    "lint:src": "tslint --project tsconfig.json",
    "lint:other": "tslint --project tsconfig.tools.json",
    "test": "tslint --test test/rules/**/*",
    "prepublish": "yarn compile",
    "prepublishOnly": "yarn lint && yarn test"
  },
  "dependencies": {
    "tslint-clean-code": "^0.2.8",
    "tslint-consistent-codestyle": "^1.13.3",
    "tslint-immutable": "^4.8.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-sonarts": "^1.8.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.1",
    "del-cli": "^1.1.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.0"
  },
  "peerDependencies": {
    "tslint": "^5.11.0",
    "typescript": "^3.0.0"
  },
  "greenkeeper": {
    "label": "Greenkeeper :green_heart:"
  }
}
