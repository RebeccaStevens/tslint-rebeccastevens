{
  "name": "@rebeccastevens/tslint-rebeccastevens",
  "version": "0.1.6",
  "description": "TSLint rules I made for myself.",
  "main": "tslint-rebeccastevens.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RebeccaStevens/tslint-rebeccastevens.git"
  },
  "author": "Rebecca Stevens <rebecca.stevens@outlook.co.nz>",
  "license": "BSD-3-Clause",
  "keywords": [
    "tslint"
  ],
  "bugs": {
    "url": "https://github.com/RebeccaStevens/tslint-rebeccastevens/issues"
  },
  "homepage": "https://github.com/RebeccaStevens/tslint-rebeccastevens#readme",
  "scripts": {
    "compile": "run-s compile:generate-tslint-files compile:build",
    "compile:generate-tslint-files": "ts-node -P tsconfig.tools.json scripts/generate-tslint-files.ts",
    "compile:build": "run-s compile:build:pre compile:build:run",
    "compile:build:pre": "del 'rules'",
    "compile:build:run": "rollup -c",
    "lint": "run-p lint:ts:src lint:ts:other",
    "lint:ts:src": "tslint --project tsconfig.json",
    "lint:ts:other": "tslint --project tsconfig.tools.json",
    "test": "run-s test:pre test:run test:cleanup --continue-on-error",
    "test:pre": "tsc -p tsconfig.test.json",
    "test:run": "tslint --test test/rules/**/*",
    "test:cleanup": "del 'src/**/*.{js,js.map}'",
    "test-with-coverage": "run-s test-with-coverage:pre test-with-coverage:run --continue-on-error",
    "test-with-coverage:pre": "del '.nyc_output' 'coverage'",
    "test-with-coverage:run": "nyc yarn test",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "yarn compile",
    "prepublishOnly": "run-s lint test"
  },
  "dependencies": {
    "tslint-clean-code": "^0.2.8",
    "tslint-consistent-codestyle": "^1.13.3",
    "tslint-immutable": "^4.8.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-sonarts": "^1.8.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.1",
    "coveralls": "^3.0.2",
    "del-cli": "^1.1.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.0"
  },
  "peerDependencies": {
    "tslint": "^5.11.0",
    "typescript": "^3.0.0"
  },
  "greenkeeper": {
    "label": "Greenkeeper :green_heart:"
  }
}
